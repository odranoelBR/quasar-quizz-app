{
  "name": "quizz",
  "version": "0.0.1",
  "description": "Aplicativo de Quest√µes",
  "productName": "Quizz",
  "cordovaId": "org.cordova.quizz.app",
  "capacitorId": "",
  "author": "muniz <leonzephiro@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env QENV=development quasar dev",
    "prod": "cross-env QENV=production quasar dev",
    "build-teste": "cross-env QENV=test quasar build -m android --debug",
    "build": "cross-env QENV=production quasar build -m android --debug",
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --coverage=false",
    "test:unit:update": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch --coverage=false",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@quasar/extras": "^1.9.0",
    "animated-number-vue": "^1.0.0",
    "axios": "^0.18.1",
    "cross-env": "^7.0.2",
    "firebase": "^7.3.0",
    "firebase-tools": "^8.6.0",
    "firebaseui": "^4.2.0",
    "global": "^4.4.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.19",
    "quasar": "^1.12.13",
    "vivus": "^0.4.5",
    "vue-rollbar": "^1.0.0",
    "vuefire": "^2.2.0",
    "vuex-map-fields": "^1.4.0",
    "vuexfire": "^3.2.3"
  },
